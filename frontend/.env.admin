# ファイル: frontend/.env.admin

# 管理者専用環境設定
REACT_APP_API_URL=http://localhost:8001
REACT_APP_ENVIRONMENT=admin
REACT_APP_APP_NAME=スポーツマンシップアプリ 管理システム
REACT_APP_DEBUG_MODE=true

# 管理者認証設定
REACT_APP_ADMIN_AUTH_REQUIRED=true
REACT_APP_ADMIN_SECRET_KEY=your-admin-secret-key-here

# データベース設定（管理者用）
REACT_APP_DB_NAME=sportsmanship_admin